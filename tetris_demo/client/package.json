{
  "name": "tetristributed-client",
  "version": "1.0.0",
  "description": "Frontend for Tetristributed using React + Socket.IO",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --watchAll=false --testPathPattern 'src/.*/__tests__/.*\\.test\\.js$|tests/unit/.*\\.test\\.js$'",
    "test:integration": "react-scripts test --testPathPattern=tests/integration",
    "test:coverage": "react-scripts test --coverage",
    "test:all": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "konva": "^8.4.3",
    "nth-check": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.8.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.6.1",
    "jest": "^27.5.1",
    "msw": "^2.7.5"
  }
}
